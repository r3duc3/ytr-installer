# where you store youtube file
config.dir=/sdcard/revanced

# name for youtube original and youtube revanced
name.original=YouTube.apk
name.modified=revanced.apk

# install mode
# available option :
# - user           : install to user data
# - system         : install to system 
config.mode=user

# path to install system mode
system.path=/system/app

# if config.mode=system
system.name=YouTube